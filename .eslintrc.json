{
  "extends": [
    "eslint:recommend",
    "plugin:@typescript-eslint/recommended",
    "plugin:vitest-globals/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}