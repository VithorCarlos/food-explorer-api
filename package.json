    "@vitest/ui": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "prisma": "^7.4.0",
    "supertest": "^7.2.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
